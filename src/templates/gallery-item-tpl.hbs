
{{#each this}}
  <li class="photo-card">
    <a data-fslightbox href="{{largeImageURL}}" class="image-link">
      <img src="{{webformatURL}}" alt="" class="card-image js-image" data-source="{{largeImageURL}}"
/>
    </a>
  
    <div class="stats">
      <p class="stats-item">
        <i class="material-icons">thumb_up</i>
        {{likes}}
      </p>
      <p class="stats-item">
        <i class="material-icons">visibility</i>
        {{views}}
      </p>
      <p class="stats-item">
        <i class="material-icons">comment</i>
        {{comments}}
      </p>
      <p class="stats-item">
        <i class="material-icons">cloud_download</i>
        {{downloads}}
      </p>
    </div>
  </li>
{{/each}}